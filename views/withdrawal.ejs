  <body>
    <div class="container">
      <div class="demo-headline" style="padding:0px;">
        <h1 class="demo-logo">ATM
          <small>24 Hour Banking</small>
        </h1>
      </div> <!-- /demo-headline -->
      <div class="row">
        <div class="col-xs-12" id="screenView">
          <!-- Place <h1></h1> below -->
          <h1 class="demo-section-title text-uppercase text-center">Withdrawal Funds</h1>
          <h6 class="text-center">###-####-5456</h6>
          
           <div class="row">

            <!-- 3/4 -->
            <div class="col-xs-9">
              <div class="row">
                <div class="col-xs-4">
                  <div class="tile">
                   <button id="qc20"  class="btn btn-lg btn-primary text-uppercase btn-padding" value="20.00">Rs 20.00</button>
                  </div>

                  <div class="tile">
                   <button id="qc40"  class="btn btn-lg btn-primary text-uppercase btn-padding" value="40.00">Rs 40.00</button>
                  </div>

                </div>
                <div class="col-xs-4">
                  <div class="tile">
                   <button id="qc60"  class="btn btn-lg btn-primary text-uppercase btn-padding" value="60.00">Rs 60.00</button>
                  </div>

                  <div class="tile">
                   <button id="qc80"  class="btn btn-lg btn-primary text-uppercase btn-padding" value="80.00">Rs 80.00</a>
                  </div>
                </div>

                <div class="col-xs-4">
                  <div class="tile">
                   <button id="qc100"  class="btn btn-lg btn-primary text-uppercase btn-padding" value="100.00">Rs. 100.00</a>
                  </div>

                  <div class="tile">
                   <button id="qc200" href="#" class="btn btn-lg btn-primary text-uppercase btn-padding" value="200.00">Rs. 200.00</a>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-xs-12">
                  <h6>Click a button to get that amount of cash.</h6>
                  <h5>or input a multiple of $20. Must be lower than $500</h5>
              <!-- <div id="qcCustomNumber" class="form-control"   >
              
              </div> -->
              <form action="/newtransaction" method="POST" >
               
                <input type="text" name="amount"  id="qcCustomNumber" class="form-control">
                <input type="text"  name="acc_no" value="<%=acc_no%>" hidden>
               
                <input type="submit" value="CONFIRM">
                </form>

              <!-- Number Keys -->

              <!-- TABLE -->
              <div id="pinPad">
                <table>
                  <tr>
                    <td><a id="btn1"  class="btn btn-block btn-lg btn-inverse" value="1">1</a></td>
                   
                    <td><a id="btn2" href="#" class="btn btn-block btn-lg btn-inverse" value="2">2</a></td>
                    <td><a id="btn3" href="#" class="btn btn-block btn-lg btn-inverse" value="3">3</a></td>
                    <td><a id="btn4" href="#" class="btn btn-block btn-lg btn-inverse" value="4">4</a></td>
                    <td><a id="btn5" href="#" class="btn btn-block btn-lg btn-inverse" value="5">5</a></td>
                   
                      <!-- <form method="POST" action="/newtransaction">
                       <input type="submit"> <td colspan="2"><a id="btnConfirm" href="#" class="btn btn-block btn-lg btn-primary text-uppercase"></a> Confirm</a></td></form>
                      </form> -->
                      
              
                  </tr>
                  <tr>
                    <td><a id="btn6" href="#" class="btn btn-block btn-lg btn-inverse" value="6">6</a></td>
                    <td><a id="btn7" href="#" class="btn btn-block btn-lg btn-inverse" value="7">7</a></td>
                    <td><a id="btn8" href="#" class="btn btn-block btn-lg btn-inverse" value="8">8</a></td>
                    <td><a id="btn9" href="#" class="btn btn-block btn-lg btn-inverse" value="9">9</a></td>
                    <td><a id="btn0" href="#" class="btn btn-block btn-lg btn-inverse" value="0">0</a></td>
                    <td colspan="2"><a id="btnDelete" href="#" class="btn btn-block btn-lg btn-default text-uppercase">Clear</a></td>
                  </tr>
                </table>
              </div><!-- // Pin Pad -->

                </div>
              </div>
              
            </div>
            <!-- 1/4 -->
            <div class="col-xs-3">
              <div class="tile">
               <a id="back" href="#" class="btn btn-lg btn-inverse text-uppercase btn-padding"><span class="fui-arrow-left"></span> Back</a>
              </div>
              <div class="tile">
               <a id="logout" href="#" class="btn btn-lg btn-info text-uppercase btn-padding"><span class="fui-user"></span> Logout</a>
              </div>
              <div class="tile">
               <a id="cancelTransactin" href="#" class="btn btn-lg btn-danger text-uppercase btn-padding"><span class="fui-cross"></span> Cancel</a>
              </div>
            </div>

        

  
